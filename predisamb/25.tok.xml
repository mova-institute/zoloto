        <w>вовну</w>
        <w>випарюють</w>
        <w>у</w>
        <w>гарячій</w>
        <w>воді</w>
        <w>З</w>
        <g/>
        <w>-</w>
        <g/>
        <w>5</w>
        <w>годин</w>
        <g/>
        <w>,</w>
        <w>потім</w>
        <w>промивають</w>
        <w>в</w>
        <w>холодній</w>
        <w>річковій</w>
        <w>воді</w>
        <g/>
        <w>.</w>
        <w>Висушену</w>
