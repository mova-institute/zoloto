        <w>тонкої</w>
        <w>бавовняної</w>
        <w>матерії</w>
        <g/>
        <w>.</w>
        <w>Чумарка</w>
        <w>була</w>
        <w>темних</w>
        <w>кольорів</w>
        <g/>
        <w>:</w>
        <w>темно</w>
        <g/>
        <w>-</w>
        <g/>
        <w>синього</w>
        <g/>
        <w>,</w>
        <w>чорного</w>
        <g/>
        <w>,</w>
        <w>сірого</w>
        <g/>
        <w>.</w>
        <w>Крій</w>
