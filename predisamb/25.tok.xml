        <w>гіршу</w>
        <g/>
        <w>.</w>
        <w>Із</w>
        <w>кращої</w>
        <w>вовни</w>
        <w>виготовляють</w>
        <w>тонкі</w>
        <w>нитки</w>
        <g/>
        <w>,</w>
        <w>які</w>
        <w>скручують</w>
        <w>вдвоє</w>
        <w>і</w>
        <w>розрізають</w>
        <w>на</w>
        <w>відрізки</w>
        <w>по</w>
        <w>30</w>
        <g/>
        <w>-</w>
        <g/>
        <w>35</w>
        <w>см</w>
        <g/>
        <w>,</w>
        <w>які</w>
        <w>протягують</w>
        <w>через</w>
        <w>верстат</w>
        <w>човником</w>
        <w>через</w>
        <w>три</w>
        <w>нитки</w>
        <w>таким</w>
        <w>чином</w>
        <g/>
        <w>,</w>
        <w>щоб</w>
        <w>правий</w>
        <w>бік</w>
        <w>сукна</w>
        <w>був</w>
        <w>кошлатий</w>
        <g/>
        <w>,</w>
        <w>а</w>
        <w>лівий</w>
        <w>—</w>
        <w>гладенький</w>
        <g/>
        <w>.</w>
